{
    "name": "webvio/filament-seo",
    "description": "Complete SEO meta tag management package for Laravel Filament with polymorphic support for any model",
    "keywords": [
        "laravel",
        "filament",
        "seo",
        "meta-tags",
        "open-graph",
        "twitter-card",
        "schema-markup",
        "polymorphic"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webvio",
            "email": "info@webvio.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "filament/filament": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Webvio\\FilamentSeo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webvio\\FilamentSeo\\FilamentSeoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
